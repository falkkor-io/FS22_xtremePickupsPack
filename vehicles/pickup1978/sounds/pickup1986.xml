<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<sounds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/vehicle_sounds.xsd">
    <motorized>
        <sounds>
            <motorStart template="CAR_MOTOR_START"  file="vehicles/pickup1978/sounds/lizard_pickup_1986_start.wav"     volumeScale="2.2"/>
            <motorStop template="CAR_MOTOR_STOP"    file="vehicles/pickup1978/sounds/lizard_pickup_1986_stop.wav"      volumeScale="2.2"/>
            <motor template="CAR_MOTOR_IDLE"        file="vehicles/pickup1978/sounds/lizard_pickup_1986_idle_loop.wav" volumeScale="2.2"/>
            <motor template="CAR_MOTOR_OFFLOAD_RUN" file="vehicles/pickup1978/sounds/lizard_pickup_1986_run_loop.wav"  volumeScale="2.2"/>
            <motor template="CAR_MOTOR_ONLOAD_RUN"  file="vehicles/pickup1978/sounds/lizard_pickup_1986_load_loop.wav" volumeScale="2.2"/>
            <gearbox template="transmissionMedium" file="vehicles/pickup1978/sounds/transmissionLoop.wav" pitchScale="0.8"   volumeScale="0.1"/>
            <gearLeverStart template="gearDisengaged"/>
            <gearLeverEnd template="gearEngaged"/>
        </sounds>
    </motorized>

    <lights>
        <sounds>
            <toggleLights template="SWITCH_01"  />
            <turnLight template="SWITCH_TURNLIGHT"  />
        </sounds>
    </lights>

    <attacherJoints>
        <sounds>
            <attach template="ATTACH_01" />
            <hydraulic template="defaultHydraulicSound" />
        </sounds>
    </attacherJoints>

    <drivable>
        <sounds>
            <waterSplash template="WATER_SPLASH_01" />
        </sounds>
    </drivable>

    <honk>
        <sound template="HONK_PICKUP" />
    </honk>
</sounds>
